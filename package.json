{
  "name": "api-wouterdeschuyter",
  "version": "0.1.0",
  "private": true,
  "repository": "github:wouterds/api.wouterdeschuyter.be",
  "author": "Wouter De Schuyter <wouter.de.schuyter@gmail.com>",
  "scripts": {
    "start": "node server",
    "dev": "nodemon",
    "sync:models": "ts-node -r ./src/bootstrap ./src/scripts/sync-models",
    "server:dev": "ts-node -r ./src/bootstrap ./src/server",
    "lint": "standardx './src/**/*.ts' --verbose | snazzy",
    "lint:fix": "standardx './src/**/*.ts' --fix && prettier './src/**/*.ts' --write",
    "build": "webpack --config ./webpack.config.ts"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.3",
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "graphql-tools": "^4.0.5",
    "mariadb": "^2.1.1",
    "sequelize": "^5.18.4",
    "tsconfig-paths": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.4",
    "@types/webpack": "^4.39.1",
    "@types/webpack-node-externals": "^1.6.3",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "graphql-tag": "^2.10.1",
    "json-loader": "^0.5.7",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "snazzy": "^8.0.0",
    "standardx": "^5.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-node-externals": "^1.7.2"
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
